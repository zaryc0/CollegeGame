<Window x:Class="TUC.Ui.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TUC.Ui.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <RadialGradientBrush RadiusY="0.573" RadiusX="0.667" GradientOrigin="0,0">
                <GradientStop Color="Aquamarine" Offset="0.91"/>
                <GradientStop Color="BlueViolet" Offset="0.3"/>
            </RadialGradientBrush>
        </Grid.Background>
        
    </Grid>
</Window>
